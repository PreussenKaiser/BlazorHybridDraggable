<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:blazor="clr-namespace:BlazorHybridDraggable.Components;assembly=BlazorHybridDraggable.Components"
             x:Class="BlazorHybridDraggable.Mobile.MainPage"
             BackgroundColor="{DynamicResource PageBackgroundColor}">

    <BlazorWebView HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type blazor:Main}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>

</ContentPage>